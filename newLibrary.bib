@article{Palme:2008ee,
  title = {Parental Income, Lifetime Income, and Mortality},
  author = {Palme, Mårten and Sandgren, Sofia},
  date = {2008-06},
  journaltitle = {Journal of the European Economic Association},
  volume = {6},
  number = {4},
