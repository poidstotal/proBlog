@article{Penner:2008tf,
  title = {Are Baby Boomers Saving Enough for Their Retirement?},
  author = {Penner, Rudolph G},
  date = {2008-11},
  pages = {1--34},
  url = {http://www.urban.org/sites/default/files/publication/32171/411794-Are-Baby-Boomers-Saving-Enough-for-Their-Retirement-.PDF},
  abstract = {This paper estimates the ratio of post- to pre-retirement consumption to explore how well boomers are prepared for retirement. I show that some of the poorest households are best prepared because they can maintain consumption by relying almost solely on Social Security while many of the most affluent households are poorly prepared because they will experience a decline in consumption upon retiring. Nonetheless, affluent households will be able to maintain a consumption level many times that of poor households. The paper discusses whether equalization of pre- and post-retirement consumption provides a useful adequacy yardstick at all income levels.},
  rating = {0}
}

