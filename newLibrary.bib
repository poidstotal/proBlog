@article{Perrenoud:1987fm,
  title = {Où va la démographie historique ? Analyse du contenu de la Bibliographie internationale de la démographie historique},
  author = {Perrenoud, Alfred},
  date = {1987},
  journaltitle = {Annales de démographie historique},
  volume = {1986},
  number = {1},
  pages = {251--272},
  doi = {10.3406/adh.1987.1663},
  url = {http://www.persee.fr/docAsPDF/adh_0066-2062_1987_num_1986_1_1663.pdf},
  abstract = {Au travers d'une analyse de contenu de la Bibliographie internationale de la démographie historique, cette étude vise à rendre compte de l'évolution récente de la recherche en démographie historique, de ses orientations, aussi bien sous l'angle des thèmes abordés que des périodes couvertes ou des régions concernées. L'analyse porte sur les années 1979-1984 et s'ordonne donc autour de trois axes : la périodisation qui révèle une montée en puissance des études s'étendant sur l'époque contemporaine ; la thématique qui semble indiquer une tendance à la spécialisation des domaines de recherches et une dérive vers une problématique plus spécifiquement démographique ; les espaces géographiques concernés qui témoignent de l'expansion mondiale de la démographie historique. Cet élargissement de l'espace s'accompagne aussi d'un déplacement des centres d'intérêt, ce qui a contribué à modifier les problématiques et les orientations de recherche.},
  langid = {fre},
  rating = {0}
}

