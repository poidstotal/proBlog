@article{Ruggeri:2007hh,
  title = {The Fiscal Burden of Rising Dependency Ratios},
  author = {Ruggeri, Joe and Zou, Yang},
  date = {2007-04},
  journaltitle = {Population Research and Policy Review},
  volume = {26},
  number = {2},
  pages = {185--201},
  doi = {10.2307/40230971?ref=search-gateway:fb5ad07f7e4b34a2ec160a88e5bb5a33},
  url = {http://download.springer.com/static/pdf/763/art%253A10.1007%252Fs11113-007-9027-8.pdf?originUrl=http%3A%2F%2Flink.springer.com%2Farticle%2F10.1007%2Fs11113-007-9027-8&token2=exp=1491182535textbackslashtextasciitildeacl=%2Fstatic%2Fpdf%2F763%2Fart%25253A10.1007%25252Fs11113-007-9027-8.pdf%3ForiginUrl%3Dhttp%253A%252F%252Flink.springer.com%252Farticle%252F10.1007%252Fs11113-007-9027-8*textbackslashtextasciitildehmac=af600909e2556589acf7401a3f61c1b1e6b3a140d61b77fefc59da9d8f985d95},
  abstract = {This paper explores the issue of the fiscal burden of population aging in Canada over the period from 2004 to 2026 by introducing a new methodological tool and using an approach that (a) is comprehensive with respect to government revenues and expenditures, (b) uses a fine disaggregation of government revenues and expenditures, and (c) integrates demographic, economic, and fiscal developments. The results indicate that the concerns about the ability of the existing fiscal system to withstand the pressures of population aging are unfounded. They indicate that, in the absence of an economic collapse, the existing fiscal structure will be able to handle the fiscal pressures of population aging. Instead of letting a nonexistent problem drive the course of fiscal policy in Canada, it would be more profitable to focus on avenues for making the fiscal structure a more effective tool for economic growth and human development.},
  langid = {english},
  rating = {0}
}

