@article{Dao:2014fl,
  title = {Le rôle de la fiscalité dans la réduction des inégalités : doit-on se soucier de la structure fiscale servant à prélever les recettes?},
  author = {Dao, Ha and Godbout, Luc},
  date = {2014},
  journaltitle = {L'Actualité économique},
  volume = {90},
  number = {4},
  pages = {303--327},
  issn = {0001-771X},
  doi = {10.7202/1035111ar},
  url = {http://id.erudit.org/iderudit/1035111ar},
  abstract = {Au cours des dernières décennies, de nombreux gouvernements, concernés par les questions de redistribution des revenus, ont porté une attention particulière à la manière dont les recettes fiscales sont collectées. Cet article applique un modèle économétrique aux données de 34 États membres de l’OCDE sur la période allant de 1970 à 2010. Il souligne que le poids des recettes fiscales dans l’économie compte bien davantage dans la réduction des inégalités que les caractéristiques de la structure fiscale servant à les collecter. Collecter plus de recettes fiscales permet aux gouvernements d’offrir davantage de services publics et de transferts à ses citoyens. Ce résultat est cohérent avec la littérature qui reconnaît que la réduction des inégalités de revenus est principalement le résultat de dépenses publiques et de transferts.},
  langid = {french},
  rating = {0}
}

