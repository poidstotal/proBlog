@article{Lesthaegh:1983in,
  title = {A Century of Demographic and Cultural Change in Western Europe: {{An}} Exploration of Underlying Dimensions},
  author = {Lesthaegh, Ron J},
  date = {1983-09},
  journaltitle = {Population and Development Review},
  volume = {9},
  number = {3},
  eprint = {1973316},
  eprinttype = {jstor},
  pages = {411--435},
  doi = {10.2307/1973316?ref=search-gateway:bfb87c98aa1fd2c2223d59dd2dc2353a},
  abstract = {Recent developments in patterns of family life in Western Europe can be related to historical features pertaining to the demographic transition and cultural change. Factor analyses performed on a set of such indicators for regional aggregates show that at least a century-old basic dimension of traditionalism versus nonconformism emerges each time new behavioral trends take off. The current developments with respect to family life are therefore interpreted as a continuation of a long historical process that has its roots in the shift of the Western ideational system to further secular individualism. This cultural interpretation leaves room for trend accelerations linked to economic conditions, but does not consider the recent demographic changes with respect to family formation as temporary phenomena occasioned by the weakened economic position of the younger cohorts.},
  langid = {english},
  rating = {0}
}

