@article{Darmon:1991fw,
  title = {Consommation : l'effet du vieillissement},
  author = {Darmon, Dominique and Hourriez, Jean-Michel and L'Hardy, Philippe},
  date = {1991},
  journaltitle = {Economie et statistique},
  volume = {243},
  number = {1},
