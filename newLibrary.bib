@article{Reynolds:2013ts,
  title = {The Incidence and Persistence of Youth Underemployment},
  author = {Reynolds, Layton},
  date = {2013-01},
  journaltitle = {The Public Policy and Governance Review PPGR},
  volume = {4},
  number = {1},
  pages = {1--14},
  url = {https://ppgr.files.wordpress.com/2012/12/ppgr-volume-4-issue-1-2-layton-reynolds1.pdf},
  abstract = {This paper presents the incidence, causes, and consequences of youth underemployment in Canada. Underemployment includes unemployment, involuntary part-time employment, over-qualification, involuntary temporary employment, and low-wage employment. In all forms of underemployment examined, underemployment rates were significantly higher for persons aged 15-24 than for persons aged 25-54 during the 1997 to 2011 period. Although underemployment declines over time, there is strong evidence that underemployment leaves workers with permanent “scars,” including psychological distress, poor physical health, lower future wages, and slower career progress. Policymakers would benefit from further research and analysis that improves our understanding of the direction of causality of different types of underemployment and their adverse psychological and social effects.},
  rating = {0}
}

