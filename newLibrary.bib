  pages = {81--92},
  publisher = {{Springer Berlin Heidelberg}},
  url = {http://link.springer.com/chapter/10.1007/978-3-642-17472-8_7},
  abstract = {Compared to other forms of savings for old age, homeownership offers some advantages. Purchasing a home is similar to purchasing an annuity that would insure housing consumption. Moreover, the home may be seen as a secure asset in case of need and perceived as a substitute for the purchase of long term care insurance. It is also a family asset that may be transmitted to the next generation. These advantages are weighted against the drawbacks of over consumption in old age (for those who are house rich and cash poor), low portfolio diversification (the price risk may be important if all assets are in the home), and illiquidity (drawing equity in case of need is not easy).},
  isbn = {978-3-642-17471-1 978-3-642-17472-8},
  keywords = {Aging,Geriatrics/Gerontology,Labor Economics,Population Economics,Public Health,Social policy.}
}

