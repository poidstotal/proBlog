@article{Kok:2007fm,
  title = {Principles and Prospects of the Life Course Paradigm},
  author = {Kok, Jan},
  date = {2007},
  journaltitle = {Annales de démographie historique},
  volume = {113},
  number = {1},
  pages = {203--230},
  issn = {0066-2062},
  doi = {10.3917/adh.113.0203},
  url = {http://www.cairn.info/revue-annales-de-demographie-historique-2007-1-page-203.htm},
  abstract = {¡titre¿R\&\#233;sum\&\#233;¡/titre¿La d\&\#233;mographie historique semble avoir pris un tournant \&\#171; micro \&\#187; en se concentrant de plus en plus sur l\&\#8217;analyse des parcours de vie. Si l\&\#8217;on suit ses avocats, il ne s\&\#8217;agit de rien moins que d\&\#8217;un nouveau paradigme de la d\&\#233;mographie (historique). Cet article propose une vue critique de cette affirmation, en examinant les succ\&\#232;s en termes de recherche et en discutant les obstacles potentiels \&\#224; la r\&\#233;ussite des promesses de cette nouvelle approche.L\&\#8217;adoption de l\&\#8217;analyse des parcours de vie a d\&\#233;j\&\#224; fourni des contributions tr\&\#232;s neuves par rapport aux sujets ignor\&\#233;s de la d\&\#233;mographie historique traditionnelle (les orphelins, le service domestique dans le cycle de vie, les \&\#233;tudes compar\&\#233;es sur les dynamiques familiales). Des sujets comme le vieillissement, les migrations et la f\&\#233;condit\&\#233; ont \&\#233;galement b\&\#233;n\&\#233;fici\&\#233; de la collecte et de l\&\#8217;analyse des donn\&\#233;es du parcours de vie.En d\&\#233;pit de ces avanc\&\#233;es, le d\&\#233;veloppement de cette approche peut \&\#234;tre compromis par plusieurs facteurs : le concept lui-m\&\#234;me, les m\&\#233;thodes statistiques utilis\&\#233;es et les pratiques de recherches dominantes ainsi que l\&\#8217;attraction de notre champ. Tout d\&\#8217;abord l\&\#8217;affirmation de la capacit\&\#233; d\&\#8217;organiser sa propre vie est d\&\#8217;application difficile dans les recherches historiques. N\&\#233;anmoins, l\&\#8217;ignorance des motifs et des buts des individus ne devrait pas dissuader d\&\#8217;identifier quels \&\#233;taient les plus probables dans certains lieux et certaines p\&\#233;riodes. Ensuite, la m\&\#233;thode de l\&\#8217;\&\#171; event history analysis \&\#187; ne permet pas de clarifier les parcours de vie en tant que processus. Des mod\&\#232;les plus \&\#233;labor\&\#233;s devraient permettre de pr\&\#233;ciser l\&\#8217;impact des exp\&\#233;riences pass\&\#233;es, des conduites alternatives, des interactions entre les processus et les contextes. Enfin, le co\&\#251;t d\&\#8217;apprentissage de la d\&\#233;mographie historique risque de devenir trop \&\#233;lev\&\#233;, en particulier si des techniques encore plus pouss\&\#233;es doivent \&\#234;tre utilis\&\#233;es. Des \&\#233;quipes interdisciplinaires pourraient r\&\#233;soudre cette difficult\&\#233;. Par ailleurs, l\&\#8217;approche du parcours de la vie suppose des bases de donn\&\#233;es compliqu\&\#233;es et tr\&\#232;s riches. Tout cela ne peut \&\#234;tre accompli par les \&\#233;quipes universitaires traditionnelles et les chercheurs solitaires. Dans ce domaine, les pratiques de recherche (et d\&\#8217;apprentissage) doivent s\&\#8217;orienter vers le partage de donn\&\#233;es, la collaboration et l\&\#8217;exploitation de banques de donn\&\#233;es publiques. Finalement, afin d\&\#8217;attirer les chercheurs d\&\#8217;autres domaines, la recherche doit privil\&\#233;gier des questions \&\#224; fort enjeu politique aujourd\&\#8217;hui comme le niveau de la f\&\#233;condit\&\#233; (enjeu du remplacement), les migrations et l\&\#8217;int\&\#233;gration des populations \&\#233;trang\&\#232;res.},
  langid = {french},
  rating = {0}
}

